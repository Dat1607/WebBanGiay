@model List<_2001200295_DuongThanhDat_WebBanGiayTheThao.Identity.AppUser>
@{
    ViewBag.Title = "Users";
}

<h2>Users</h2>

<table class="table table-bordered">
    <tr>
        <th>UserName</th>
        <th>Email</th>
        <th>Mobile</th>
        <th>City</th>
        <th>Date of Birth</th>

    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.UserName</td>
            <td>@item.Email</td>
            <td>@item.PhoneNumber</td>
            <td>@item.City</td>
            <td>@item.Birthday</td>
        </tr>
    }
</table>